.header-cell {
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 27rpx;
}

.header-cell .avatar {
  width: 150rpx;
  height: 150rpx;
  display: block;
  padding: 10rpx;

}
.cell {
  border-bottom: 1rpx solid rgb(228,228,228);
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  font-size: 26rpx;
}
.cell .order {
  flex-grow: 1;
}

